Sub Rounding_two()
    For i = 1 To range("A1").CurrentRegion.Cells.count
        If IsNumeric(range("A1").CurrentRegion.Cells(i)) Then
            range("A1").CurrentRegion.Cells(i) = Round(range("A1").CurrentRegion.Cells(i), 2)
        End If
    Next i
End Sub


